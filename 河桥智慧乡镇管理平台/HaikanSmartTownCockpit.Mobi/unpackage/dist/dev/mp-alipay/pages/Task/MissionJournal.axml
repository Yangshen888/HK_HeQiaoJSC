s="{{[['input',[['__set_model',['$0','content','$event',[]],['formmodelrizhi']]]]]}}" value="{{formmodelrizhi.content}}" onInput="__e"/><text data-event-opts="{{[['tap',[['ChooseImage',['$event']]]]]}}" class="lg text-gray cuIcon-camerafill" style="margin-right:3px;" onTap="__e"></text><button data-event-opts="{{[['tap',[['dogetscorelist',['$event']]]]]}}" class="cu-btn bg-green shadow" onTap="__e">发送</button></view><view class="cu-form-group"><view class="grid col-4 grid-square flex-sub"><view class="bg-img" data-url="{{imgList[index]}}" data-event-opts="{{[['tap',[['ViewImage',['$event']]]]]}}" onTap="__e" a:for="{{imgList}}" a:for-item="item" a:for-index="index" a:key="index"><image src="{{imgList[index]}}" mode="aspectFill"></image><view class="cu-tag bg-red" data-index="{{index}}" data-event-opts="{{[['tap',[['DelImg',['$event']]]]]}}" catchTap="__e"><text class="cuIcon-close"></text></view></view></view></view></view></form></view>