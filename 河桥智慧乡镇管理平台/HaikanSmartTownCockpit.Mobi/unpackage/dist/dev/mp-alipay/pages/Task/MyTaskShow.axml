<view class="cu-list menu"><view class="cu-bar search bg-white"><view class="search-form round"><text class="cuIcon-search"></text><input adjust-position="{{false}}" type="text" placeholder="输入名称,负责人,结束时间" confirm-type="search" data-event-opts="{{[['input',[['__set_model',['','searhdata','$event',[]]]]]]}}" value="{{searhdata}}" onInput="__e"/></view><view class="action"><button data-event-opts="{{[['tap',[['searchclick',['$event']]]]]}}" class="cu-btn bg-cyan margin-tb-sm" onTap="__e">搜索</button></view></view><view class="cu-item" style="text-align:center;"><view class="content"><text class="text-grey" style="font-weight:bold;">名称</text></view><view class="content"><text class="text-grey" style="font-weight:bold;">负责人</text></view><view class="content"><text class="text-grey" style="font-weight:bold;">结束时间</text></view><view class="content"><text class="text-grey" style="font-weight:bold;">操作</text></view></view><view class="cu-item" style="text-align:center;" a:for="{{$root.l0}}" a:for-item="item" a:for-index="index"><view class="content"><text class="text-grey">{{item.$orig.misname}}</text></view><view class="content"><text class="text-grey">{{item.$orig.fuzerename.length>5?item.g0+"...":item.$orig.fuzerename}}</text></view><view class="content"><text class="text-grey">{{item.$orig.endtime}}</text></view><view class="content"><block a:if="{{item.m0}}"><button data-event-opts="{{[['tap',[['Accomplish',['$0'],[[['datalist','',index,'renwuuuid']]]]]]]}}" class="cu-btn bg-cyan margin-tb-sm" onTap="__e">{{btnname}}</button></block><button data-event-opts="{{[['tap',[['MissionJournal',['$0'],[[['datalist','',index,'renwuuuid']]]]]]]}}" class="cu-btn bg-cyan margin-tb-sm" onTap="__e">个人日志详细</button></view></view></view>